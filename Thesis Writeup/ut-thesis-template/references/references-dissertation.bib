@article{roberge_comparison_2013,
	title = {Comparison of {Parallel} {Genetic} {Algorithm} and {Particle} {Swarm} {Optimization} for {Real}-{Time} {UAV} {Path} {Planning}},
	volume = {9},
	issn = {1551-3203},
	doi = {10.1109/TII.2012.2198665},
	abstract = {The development of autonomous unmanned aerial vehicles (UAVs) is of high interest to many governmental and military organizations around the world. An essential aspect of UAV autonomy is the ability for automatic path planning. In this paper, we use the genetic algorithm (GA) and the particle swarm optimization algorithm (PSO) to cope with the complexity of the problem and compute feasible and quasi-optimal trajectories for fixed wing UAVs in a complex 3D environment, while considering the dynamic properties of the vehicle. The characteristics of the optimal path are represented in the form of a multiobjective cost function that we developed. The paths produced are composed of line segments, circular arcs and vertical helices. We reduce the execution time of our solutions by using the “single-program, multiple-data” parallel programming paradigm and we achieve real-time performance on standard commercial off-the-shelf multicore CPUs. After achieving a quasi-linear speedup of 7.3 on 8 cores and an execution time of 10 s for both algorithms, we conclude that by using a parallel implementation on standard multicore CPUs, real-time path planning for UAVs is possible. Moreover, our rigorous comparison of the two algorithms shows, with statistical significance, that the GA produces superior trajectories to the PSO.},
	number = {1},
	journal = {IEEE Transactions on Industrial Informatics},
	author = {Roberge, V. and Tarbouchi, M. and Labonte, G.},
	month = feb,
	year = {2013},
	keywords = {automatic path planning, autonomous aerial vehicles, autonomous unmanned aerial vehicle, commercial off-the-shelf multicore CPU, control engineering computing, Cost function, dynamic property, fixed wing UAV, Fuels, Genetic algorithm (GA), Genetic algorithms, multiobjective cost function, multiple-data parallel programming paradigm, parallel computing, parallel genetic algorithm, parallel programming, particle swarm optimisation, particle swarm optimization algorithm, particle swarm optimization (PSO), path planning, quasioptimal trajectory, robot programming, single-program parallel programming paradigm, Three dimensional displays, Trajectory, UAV autonomy, UAV path planning, unmanned aerial vehicles (UAVs)},
	pages = {132--141},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\8FET97QG\\6198334.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\QC7G6XQN\\Roberge et al. - 2013 - Comparison of Parallel Genetic Algorithm and Parti.pdf:application/pdf}
}

@article{ruiz_biased_2015,
