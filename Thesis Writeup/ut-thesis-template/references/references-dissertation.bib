	file = {ScienceDirect Full Text PDF:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\FN6FWWCU\\Devos et al. - 2014 - Simultaneous data pre-processing and SVM classific.pdf:application/pdf;ScienceDirect Full Text PDF:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\FWQH66IF\\Devos et al. - 2014 - Simultaneous data pre-processing and SVM classific.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\ASSSU8S5\\S0308814613014520.html:text/html;ScienceDirect Snapshot:C\:\\Users\\Isaac\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\zqu9roww.default\\zotero\\storage\\Z9ET49HK\\S0308814613014520.html:text/html}
}

@article{uysal_text_2014,
	title = {Text classification using genetic algorithm oriented latent semantic features},
	volume = {41},
	issn = {0957-4174},
	url = {http://www.sciencedirect.com/science/article/pii/S0957417414001791},
	doi = {10.1016/j.eswa.2014.03.041},
	abstract = {In this paper, genetic algorithm oriented latent semantic features (GALSF) are proposed to obtain better representation of documents in text classification. The proposed approach consists of feature selection and feature transformation stages. The first stage is carried out using the state-of-the-art filter-based methods. The second stage employs latent semantic indexing (LSI) empowered by genetic algorithm such that a better projection is attained using appropriate singular vectors, which are not limited to the ones corresponding to the largest singular values, unlike standard LSI approach. In this way, the singular vectors with small singular values may also be used for projection whereas the vectors with large singular values may be eliminated as well to obtain better discrimination. Experimental results demonstrate that GALSF outperforms both LSI and filter-based feature selection methods on benchmark datasets for various feature dimensions.},
	number = {13},
